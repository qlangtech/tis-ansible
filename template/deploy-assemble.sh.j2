#!/bin/bash

mvn_bin="{{mvn_home}}/bin/mvn"
parent_dir=`pwd`

for m in {{tisasm_put_mojo}}
do
  cd $parent_dir/$m
  echo "start deploy $m"
  $mvn_bin com.qlangtech.tis:tisasm-maven-plugin:1.0.4:put -Drelease_dir={{tis_release_dir}}/tis -Dappname=all -Dtisasm.oss.put.skip=false
  echo "success deploy $m"
done